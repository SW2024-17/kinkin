<tr>
  <td><%= attendance.check_in %></td>
  <td><%= attendance.check_out || '未退勤' %></td>
  <td><%= attendance.status %></td>
  <td>
    <% if attendance.check_out.nil? %>
      <%= link_to '退勤', check_out_user_attendance_path(attendance.user, attendance), method: :patch, data: { confirm: '退勤処理を行いますか？' } %>
    <% end %>
  </td>
</tr>
